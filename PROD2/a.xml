<operatingSystemRoot id="linux01" family="unix" vendor="community">
  <kernelCore type="monolithic" version="6.6">
    <moduleExt4>
      <ext4Status>enabled</ext4Status>
    </moduleExt4>
    <moduleBtrfs>
      <btrfsState>enabled</btrfsState>
    </moduleBtrfs>
  </kernelCore>
  <serviceSSH>
    <sshPort>2222</sshPort>
    <sshPolicy>allow</sshPolicy>
  </serviceSSH>
  <securityPolicy>
    <selinuxMode>permissive</selinuxMode>
  </securityPolicy>
  <observabilityAgent>
    <agentName>node_exporter</agentName>
  </observabilityAgent>
  <resourceLimits>
    <maxOpenFiles>
      <softLimit>4096</softLimit>
    </maxOpenFiles>
  </resourceLimits>
</operatingSystemRoot>
